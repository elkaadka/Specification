{
    "name": "kanel/specification",
    "description": "An implementation of the Specification design pattern",
    "license": "MIT",
    "require" : {
        "php" : ">=7.0.0"
    },
    "repositories" : [ {
        "type" : "composer",
        "url" : "https://packagist.org/"
    } ],
    "require-dev" : {
        "phpunit/phpunit" : "6.0.8",
        "phpspec/phpspec": "^3.4"
    },
    "autoload-dev" : {
        "psr-4" : {
            "Kanel\\Specification\\" : "src/",
            "Kanel\\Specification\\Tests\\" : "tests/fixtures"
        }
    },
    "autoload" : {
        "psr-4" : {
            "Kanel\\Specification\\" : "src/"
        }
    },
    "authors" : [ {
        "name" : "Adil El Kanabi",
        "email" : "adil.kanabi@gmail.com"
    } ]
}
